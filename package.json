{
	"name": "kodgen-cli",
	"version": "0.0.1",
	"description": "",
	"main": "dist/src/index.js",
	"bin": {
		"kodgen": "dist/src/index.js"
	},
	"scripts": {
		"build": "rimraf dist && tsc --project tsconfig.build.json",
		"test": "jest",
		"lint": "eslint src/**/*.ts",
		"format:check": "prettier --list-different \"**/*{.ts,.js,.json}\"",
		"format:write": "prettier -w --list-different \"**/*{.ts,.js,.json}\"",
		"prepack": "npm run build",
		"commitlint": "commitlint",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MacRdy/kodgen-cli.git"
	},
	"author": "Anton Shibaev",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/MacRdy/kodgen-cli/issues"
	},
	"homepage": "https://github.com/MacRdy/kodgen-cli#readme",
	"files": [
		"dist/**/*"
	],
	"peerDependencies": {
		"kodgen": "file:kodgen-0.0.1.tgz"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.7.1",
		"@commitlint/config-conventional": "^17.7.0",
		"@types/jest": "^29.5.4",
		"@types/yargs": "^17.0.24",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"eslint": "^8.47.0",
		"eslint-plugin-optimize-regex": "^1.2.1",
		"eslint-plugin-sonarjs": "^0.20.0",
		"husky": "^8.0.3",
		"jest": "^29.6.4",
		"prettier": "^3.0.2",
		"rimraf": "^3.0.2",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"ajv": "^8.12.0",
		"openapi-types": "^12.1.3",
		"yargs": "^17.7.2"
	}
}
